@{ ViewBag.Title = "UKPRN Not Found"; }

<form asp-controller="AddRoatpOrganisation" asp-action="Back" method="post" name="backForm">
    <input type="hidden" name="action" value="EnterUkprn" />
    <a href="JavaScript:document.backForm.submit();" class="govuk-back-link">Back</a>
</form>

<main class="govuk-main-wrapper " id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-xl">UKPRN not found</h1>
        
            <p class="govuk-body">This is because the UKPRN either:</p>

            <ul class="govuk-list govuk-list--bullet">
                <li>does not exist</li>
                <li>is inactive on the UK Register of Learning Providers</li>
            </ul>

            <form method="post" asp-action="EnterUkprn" asp-controller="AddRoatpOrganisation">
                <p class="govuk-body">
                    <a class="govuk-link" asp-controller="AddRoatpOrganisation" asp-action="EnterUkprn">
                        Try again
                    </a>
                </p>
            </form>
        </div>
    </div>
</main>


